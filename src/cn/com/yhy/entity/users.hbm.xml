<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
       <hibernate-mapping package="cn.com.yhy.entity">
       		<class name="Users" table="USERS">
       			<id name="sid">
       					<generator class="increment"/>
       			</id>
       			<property name="username" not-null="true" unique="true"></property>
       			<property name="password" not-null="true"/>
       			<property name="concat" not-null="true"/>
       			<property name="company" not-null="true"/>
       			<property name="phone" not-null="true"/>
       			<property name="qq" not-null="true"/>
       		</class>
       </hibernate-mapping>